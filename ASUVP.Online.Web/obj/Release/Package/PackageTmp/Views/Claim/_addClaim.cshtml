@using ASUVP.Core.DataAccess.Model
@model IEnumerable<TemplateGetClaim_Result>
@using (Html.BeginForm("Action",
                        "Controller",
                        new { ReturnUrl = ViewBag.ReturnUrl },
                        FormMethod.Post, new { @class = "addForm" }))
{
<p>Выбирете шаблон</p>
<select class="template">
   @foreach (var item in Model)
   {
    <option value="@item.Id">@item.Name</option>
   }

</select>
<br>
<input class="btn sbmt" type="submit" value="Открыть">
}