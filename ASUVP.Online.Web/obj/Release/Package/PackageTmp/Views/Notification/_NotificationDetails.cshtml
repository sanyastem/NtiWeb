@using System.Web.UI.WebControls
@using ASUVP.Core.DataAccess.Model
@model UserNotificationList
<div id="content-inner" style="padding: 10px 30px 30px 30px; background: #ffffff; width: 100%">
    <div class="container-fluid">

        @*<div class="btn-group btn-group-header row" style="padding-left:4px">
                <input type="submit" class="btn btnSave" value="Сохранить">
            </div>*@

        <table style="width:100%">
            <tr>
                <td class="left-label">
                    Название:
                </td>
                <td class="one-inrow-editor">
                    @Model.
                </td>
            </tr>
            <tr>
                <td class="left-label">
                    Тип письма:
